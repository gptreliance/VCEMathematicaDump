Cell[TextData[{
 StyleBox["Without",
  FontWeight->"Plain"],
 " Reals"
}], "Subsection",
 CellChangeTimes->{{3.9531441526350307`*^9, 
  3.953144157592119*^9}},ExpressionUUID->"a3134e7d-5d96-5542-80d9-\
59e1354fc137"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"Solve", "[", 
  RowBox[{
   RowBox[{
    SuperscriptBox["\[ExponentialE]", 
     RowBox[{
      RowBox[{"3", "x"}], "-", "2"}]], "==", "4"}], ",", "x"}], 
  "]"}]], "Input",
 CellChangeTimes->{{3.9531441621273575`*^9, 3.9531441777693386`*^9}},
 CellLabel->"In[14]:=",ExpressionUUID->"bd858b2b-8e64-1342-aeb3-d43bfd1a4e2b"],

Cell[BoxData[
 RowBox[{"{", 
  RowBox[{"{", 
   RowBox[{"x", "\[Rule]", 
    TemplateBox[{
      RowBox[{
        FractionBox["1", "3"], " ", 
        RowBox[{"(", 
          RowBox[{"2", "+", 
            RowBox[{"2", " ", "\[ImaginaryI]", " ", "\[Pi]", " ", 
              TemplateBox[{"1"}, "C"]}], "+", 
            RowBox[{"Log", "[", "4", "]"}]}], ")"}]}], 
      RowBox[{
        TemplateBox[{"1"}, "C"], "\[Element]", 
        TemplateBox[{}, "Integers"]}]},
     "ConditionalExpression"]}], "}"}], "}"}]], "Output",
 CellChangeTimes->{3.9531441788733025`*^9},
 CellLabel->"Out[14]=",ExpressionUUID->"a78bce20-8c4e-bd40-aa83-b17d332589a6"]
}, Open  ]]
}, Open  ]],

