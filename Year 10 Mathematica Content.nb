     RowBox[{
      RowBox[{"f", "[", 
       RowBox[{"-", "1"}], "]"}], "==", "10"}], ",", 
     RowBox[{
      RowBox[{"f", "[", "3", "]"}], "==", 
      RowBox[{"-", "2"}]}]}], "}"}], ",", 
   RowBox[{"{", 
    RowBox[{"a", ",", "b", ",", "c"}], "}"}]}], "]"}]], "Input",
