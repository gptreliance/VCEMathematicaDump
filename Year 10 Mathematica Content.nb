Cell[TextData[{
 "Remember to ",
 StyleBox["include Reals",
  FontWeight->"Bold"],
 " to yield a ",
 StyleBox["real number",
  FontWeight->"Bold"],
 "."
}], "Text",
 CellChangeTimes->{{3.9531441293643093`*^9, 
  3.95314413860075*^9}},ExpressionUUID->"c1f3dab4-501b-4c44-bfdc-\
f49d227f2bb5"],

