0035e1b3663b"],

Cell[CellGroupData[{

Cell["Solving Simultaneous Equations", "Section",
 CellChangeTimes->{{3.9516736223043404`*^9, 
  3.9516736314589653`*^9}},ExpressionUUID->"bc683743-4904-654b-b60b-\
dd955e3af8fc"],

Cell[TextData[{
 "If there are multiple things going into a single parameter, use ",
 StyleBox["curly braces",
  FontWeight->"Bold"],
 " to fit them. "
}], "Text",
 CellChangeTimes->{{3.95167372650457*^9, 
  3.951673787184622*^9}},ExpressionUUID->"c2d52592-8d3a-5144-98e2-\
1cb4619af7a2"],

Cell[TextData[{
 "e.g. When solving two or more equations, the equations must be in curly \
braces, separated by a ",
 StyleBox["comma",
  FontWeight->"Bold"],
 ". "
}], "Text",
 CellChangeTimes->{{3.95167372650457*^9, 
  3.9516738194159336`*^9}},ExpressionUUID->"2015baa3-214e-a649-98d2-\
42b732368805"],

Cell[TextData[{
 "Remember to use ",
 StyleBox["double equals ",
  FontWeight->"Bold"],
 "\[OpenCurlyDoubleQuote]==\[CloseCurlyDoubleQuote] similar to other \
programming languages."
}], "Text",
 CellChangeTimes->{{3.95167372650457*^9, 
  3.951673853313751*^9}},ExpressionUUID->"0bbd4f1d-9a02-c440-9169-\
d00150d18f71"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"Solve", "[", 
  RowBox[{
   RowBox[{"{", 
    RowBox[{
     RowBox[{"y", "==", 
      RowBox[{
       RowBox[{"2", "x"}], "-", "1"}]}], ",", 
     RowBox[{"y", "==", 
      RowBox[{"3", "-", "x"}]}]}], "}"}], ",", 
   RowBox[{"{", 
    RowBox[{"x", ",", "y"}], "}"}]}], "]"}]], "Input",
 CellChangeTimes->{{3.9516736381732464`*^9, 3.951673659089245*^9}, {
  3.951673692290512*^9, 3.951673692399086*^9}},
 CellLabel->"In[4]:=",ExpressionUUID->"ecf0307e-23b7-4746-8cb6-e0f4077ac0bb"],

Cell[BoxData[
 RowBox[{"{", 
  RowBox[{"{", 
   RowBox[{
    RowBox[{"x", "\[Rule]", 
     FractionBox["4", "3"]}], ",", 
    RowBox[{"y", "\[Rule]", 
     FractionBox["5", "3"]}]}], "}"}], "}"}]], "Output",
 CellChangeTimes->{{3.9516736620037537`*^9, 3.9516736928689632`*^9}},
 CellLabel->"Out[4]=",ExpressionUUID->"608bbdc6-926d-a742-87c9-4a2677874ffc"]
}, Open  ]],

Cell[TextData[{
 "Usage: ",
 StyleBox["Solve[{equation_a",
  FontWeight->"Plain"],
 ",",
