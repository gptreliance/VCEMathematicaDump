Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"Solve", "[", 
  RowBox[{
   RowBox[{"{", 
    RowBox[{
     RowBox[{"y", "==", 
      RowBox[{
       RowBox[{"2", "x"}], "-", "1"}]}], ",", 
     RowBox[{"y", "==", 
      RowBox[{"3", "-", "x"}]}]}], "}"}], ",", 
   RowBox[{"{", 
    RowBox[{"x", ",", "y"}], "}"}]}], "]"}]], "Input",
