  RowBox[{"f", "[", "x_", "]"}], ":=", 
  RowBox[{
   RowBox[{"a", " ", 
    SuperscriptBox["x", "2"]}], "+", 
   RowBox[{"b", " ", "x"}], "+", "c"}]}]], "Input",
 CellChangeTimes->{{3.951896136631221*^9, 3.951896165496719*^9}},
